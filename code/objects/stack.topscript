#part 1
let s = object
  val mutable v = [0; 2]

  method pop =
    match v with
    | hd :: tl -> 
       v <- tl;
       Some hd
    | [] -> None

  method push hd = 
    v <- hd :: v
end;;
#part 2
s#pop;;
s#push 4;;
s#pop;;

#part 3
let stack init = object
  val mutable v = init

  method pop =
    match v with
      hd :: tl -> 
        v <- tl;
        Some hd
    | [] -> None

  method push hd = 
    v <- hd :: v
end;;
let s = stack [3; 2; 1];;
s#pop;;
