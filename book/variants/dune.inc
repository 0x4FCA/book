(alias
 (name   runtest)
 (deps   (:x README.md)
         (:y6 ../../examples/code/variants-termcol-annotated/dune)
         (:y5 ../../examples/code/variants-termcol-annotated/dune-project)
         (:y4 ../../examples/code/variants-termcol-annotated/dune-workspace)
         (:y3 ../../examples/code/variants-termcol-annotated/terminal_color.ml)
         (:y2 ../../examples/code/variants-termcol-fixed/terminal_color.ml)
         (:y1 ../../examples/code/variants-termcol/terminal_color.ml)
         (:y0 ../../examples/code/variants-termcol/terminal_color.mli)
         ../prelude.ml
         (package mdx))
 (action (progn
           (run mdx test --prelude=../prelude.ml --direction=infer-timestamp %{x})
           (diff? %{x} %{x}.corrected)
           (diff? %{y6} %{y6}.corrected)
           (diff? %{y5} %{y5}.corrected)
           (diff? %{y4} %{y4}.corrected)
           (diff? %{y3} %{y3}.corrected)
           (diff? %{y2} %{y2}.corrected)
           (diff? %{y1} %{y1}.corrected)
           (diff? %{y0} %{y0}.corrected))))
