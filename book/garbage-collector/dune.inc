(alias
 (name   runtest)
 (deps   (:x README.md)
         (:y7 ../../examples/code/gc/barrier_bench/barrier_bench.ml)
         (:y6 ../../examples/code/gc/barrier_bench/dune)
         (:y5 ../../examples/code/gc/barrier_bench/dune-project)
         (:y4 ../../examples/code/gc/barrier_bench/dune-workspace)
         (:y3 ../../examples/code/gc/finalizer/dune)
         (:y2 ../../examples/code/gc/finalizer/dune-project)
         (:y1 ../../examples/code/gc/finalizer/dune-workspace)
         (:y0 ../../examples/code/gc/finalizer/finalizer.ml)
         prelude.ml
         (package mdx))
 (action (progn
           (run mdx test --prelude=prelude.ml --direction=to-ml %{x})
           (diff? %{x} %{x}.corrected)
           (diff? %{y7} %{y7}.corrected)
           (diff? %{y6} %{y6}.corrected)
           (diff? %{y5} %{y5}.corrected)
           (diff? %{y4} %{y4}.corrected)
           (diff? %{y3} %{y3}.corrected)
           (diff? %{y2} %{y2}.corrected)
           (diff? %{y1} %{y1}.corrected)
           (diff? %{y0} %{y0}.corrected))))
(alias
 (name   runtest-all)
 (deps   (:x README.md)
         (:y7 ../../examples/code/gc/barrier_bench/barrier_bench.ml)
         (:y6 ../../examples/code/gc/barrier_bench/dune)
         (:y5 ../../examples/code/gc/barrier_bench/dune-project)
         (:y4 ../../examples/code/gc/barrier_bench/dune-workspace)
         (:y3 ../../examples/code/gc/finalizer/dune)
         (:y2 ../../examples/code/gc/finalizer/dune-project)
         (:y1 ../../examples/code/gc/finalizer/dune-workspace)
         (:y0 ../../examples/code/gc/finalizer/finalizer.ml)
         prelude.ml
         (package mdx))
 (action (progn
           (run mdx test --prelude=prelude.ml --direction=to-ml --non-deterministic %{x})
           (diff? %{x} %{x}.corrected)
           (diff? %{y7} %{y7}.corrected)
           (diff? %{y6} %{y6}.corrected)
           (diff? %{y5} %{y5}.corrected)
           (diff? %{y4} %{y4}.corrected)
           (diff? %{y3} %{y3}.corrected)
           (diff? %{y2} %{y2}.corrected)
           (diff? %{y1} %{y1}.corrected)
           (diff? %{y0} %{y0}.corrected))))
