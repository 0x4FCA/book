(alias
 (name   runtest)
 (deps   (:x README.md)
         (:y17 ../../examples/code/ctypes/ctypes.mli)
         (:y16 ../../examples/code/ctypes/ctypes_impl.ml)
         (:y15 ../../examples/code/ffi/datetime/datetime.ml)
         (:y14 ../../examples/code/ffi/datetime/dune)
         (:y13 ../../examples/code/ffi/datetime/dune-project)
         (:y12 ../../examples/code/ffi/datetime/dune-workspace)
         (:y11 ../../examples/code/ffi/hello/dune)
         (:y10 ../../examples/code/ffi/hello/dune-project)
         (:y9 ../../examples/code/ffi/hello/dune-workspace)
         (:y8 ../../examples/code/ffi/hello/hello.ml)
         (:y7 ../../examples/code/ffi/ncurses/ncurses.ml)
         (:y6 ../../examples/code/ffi/ncurses/ncurses.mli)
         (:y5 ../../examples/code/ffi/qsort/dune)
         (:y4 ../../examples/code/ffi/qsort/dune-project)
         (:y3 ../../examples/code/ffi/qsort/dune-workspace)
         (:y2 ../../examples/code/ffi/qsort/qsort.ml)
         (:y1 ../../examples/code/ffi/qsort/qsort.mli)
         (:y0 ../../examples/code/ffi/return_frag.ml)
         prelude.ml
         (package mdx))
 (action (progn
           (run mdx test --prelude=prelude.ml --direction=to-ml %{x})
           (diff? %{x} %{x}.corrected)
           (diff? %{y17} %{y17}.corrected)
           (diff? %{y16} %{y16}.corrected)
           (diff? %{y15} %{y15}.corrected)
           (diff? %{y14} %{y14}.corrected)
           (diff? %{y13} %{y13}.corrected)
           (diff? %{y12} %{y12}.corrected)
           (diff? %{y11} %{y11}.corrected)
           (diff? %{y10} %{y10}.corrected)
           (diff? %{y9} %{y9}.corrected)
           (diff? %{y8} %{y8}.corrected)
           (diff? %{y7} %{y7}.corrected)
           (diff? %{y6} %{y6}.corrected)
           (diff? %{y5} %{y5}.corrected)
           (diff? %{y4} %{y4}.corrected)
           (diff? %{y3} %{y3}.corrected)
           (diff? %{y2} %{y2}.corrected)
           (diff? %{y1} %{y1}.corrected)
           (diff? %{y0} %{y0}.corrected))))
