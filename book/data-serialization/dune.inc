(alias
 (name   runtest)
 (deps   (:x README.md)
         (:y20 ../../examples/code/sexpr/int_interval_manual_sexp.mli)
         (:y19 ../../examples/code/sexpr/int_interval_nosexp.mli)
         (:y18 ../../examples/code/sexpr/read_foo/dune)
         (:y17 ../../examples/code/sexpr/read_foo/dune-project)
         (:y16 ../../examples/code/sexpr/read_foo/dune-workspace)
         (:y15 ../../examples/code/sexpr/read_foo/read_foo.ml)
         (:y14 ../../examples/code/sexpr/read_foo_better_errors/dune)
         (:y13 ../../examples/code/sexpr/read_foo_better_errors/dune-project)
         (:y12 ../../examples/code/sexpr/read_foo_better_errors/dune-workspace)
         (:y11 ../../examples/code/sexpr/read_foo_better_errors/read_foo_better_errors.ml)
         (:y10 ../../examples/code/sexpr/sexp.mli)
         (:y9 ../../examples/code/sexpr/sexp_override.ml)
         (:y8 ../../examples/code/sexpr/test_interval/dune)
         (:y7 ../../examples/code/sexpr/test_interval/dune-project)
